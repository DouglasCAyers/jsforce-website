---
cacheable: false
---
<article id="post" class="post">
  <div class="post-date"><%=  @moment(@date).format('MMM Do, YYYY') %></div>
  <% if @individualPost: %>
  <h2><%= @title %></h2>
  <% else: %>
  <h2><a href='<%= @url %>'><%= @title %></a></h2>
  <% end %>
  <div class="post-content">
     <%- @content %>
  </div>
</article>